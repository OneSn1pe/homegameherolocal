{
  "name": "home-game-hero",
  "version": "1.0.0",
  "description": "Simple offline poker calculator for home games",
  "main": "poker-calculator.html",
  "scripts": {
    "dev": "npx live-server --host=localhost --port=8080 --entry-file=poker-calculator.html",
    "dev-http": "npx http-server -a localhost -p 8080 -c-1 -o /poker-calculator.html",
    "start": "npm run dev",
    "serve": "npx serve -l 8080",
    "kill-port": "kill-port 8080"
  },
  "keywords": [
    "poker",
    "calculator",
    "offline",
    "pwa",
    "game"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "http-server": "^14.1.1",
    "kill-port": "^2.0.1",
    "live-server": "^1.2.2"
  }
}